body {
  font-family: Georgia, Times, serif!important;
  font-size: 16px;
  font-weight: 300;
  margin-bottom: 150px;
  letter-spacing: 1px;
  color: #333
}

.main {
  min-height: 61vh
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-family: "Georgia"!important;
  font-weight: 400;
  margin-top: 40px;
  margin-bottom: 20px
}

h1 {
  font-size: 2em;
  font-weight: 400;
  margin-top: 0
}

h2 {
  font-size: 1.8em;
  font-weight: 400
}

h3 {
  font-size: 1.5em;
  font-weight: 400;
  margin-top: 30px;
  margin-bottom: 15px
}

h4 {
  font-size: 1.2em;
  font-weight: 500
}

h5 {
  font-size: 1em;
  font-weight: 300
}

p {
  color: #444;
  font-weight: 300;
  line-height: 1.5
}

a:link,
a:visited,
a:hover,
a:active {
  color: #537FBC;
  text-decoration: none
}

a:hover {
  color: #444!important
}

pre {
  background-color: #f0f0f0;
  background-color: #eff1f5!important
}

pre code {
  font-size: 0.8em;
  line-height: auto;
  border: none
}

code {
  font-size: 0.8em;
  border: 1px solid #ccc;
  background-color: #f0f0f0;
  background-color: #eff1f5
}

.title {
  font-family: 'Open Sans', sans-serif;
  font-weight: 300;
  font-size: 1.7em;
  letter-spacing: 1.5px;
  color: #537FBC;
  color: #444;
  max-width: 700px;
  margin-bottom: 10px;
  margin-top: 50px
}

@media (min-width:768px) {
  .title {
    margin-bottom: 30px
  }
}

@media (max-width:768px) {
  .title {
    margin-bottom: 0;
    margin-top: 35px
  }
}

.menu {
  font-family: 'Open Sans', sans-serif;
  font-weight: 300;
  margin-top: 8px;
  font-size: 1.2em
}

.menu a:link,
.menu a:visited,
.menu a:hover,
.menu a:active {
  color: #444;
  text-decoration: none
}

@media (min-width:768px) {
  .menu a:link,
  .menu a:visited,
  .menu a:hover,
  .menu a:active {
    margin-left: 20px
  }
}

.menu a:hover {
  color: #444!important;
  text-decoration: underline
}

hr.top {
  width: 100vw;
  color: #999;
  height: 0.25px;
  margin-left: -25px
}

@media (min-width:768px) {
  hr.top {
    margin-top: -20px;
    margin-left: -10px
  }
}

a.navbar-header:hover {
  color: #333
}

.navbar-default {
  background-color: transparent!important;
  border: none;
  margin-top: 10px
}

@media (max-width:580px) {
  .navbar-default {
    margin-left: 15px;
    margin-right: 25px
  }
}

nav ul {
  font-size: 1.3em;
  text-align: right
}

ul {
  padding-left: 20px;
  font-weight: 200
}

.resume {
  font-family: 'Open Sans', sans-serif
}

.resume .row {
  margin-bottom: 30px
}

.resume ul {
  margin-left: 0!important;
  padding-left: 0!important;
  font-size: 1em;
  font-weight: 300
}

.resume h1,
.resume h2,
.resume h3,
.resume h4,
.resume h5,
.resume h6 {
  font-family: 'Open Sans', sans-serif!important;
  margin-top: 5px;
  margin-right: 0;
  margin-bottom: 0;
  margin-left: 0;
  padding-top: 0;
  padding-right: 0;
  padding-bottom: 0;
  padding-left: 0
}

.resume h1 {
  font-size: 1em;
  margin-top: 15px
}

.resume h2 {
  font-size: 1em
}

.resume .title {
  font-size: 1.2em;
  margin-top: 8px
}

ul.pubs {
  list-style-type: none;
  padding-left: 32px;
  text-indent: -20px;
  font-size: 1em;
  font-weight: 300
}

ul.pubs li {
  margin-bottom: 0.9em
}

ul.pubs li .this-author {
  font-weight: 500
}

ul.no-bull {
  list-style: none
}

ul.left {
  padding-left: 0
}

.no-left {
  padding-left: 0!important;
  margin-left: 0!important;
  left: 0!important
}

.para {
  color: #ccc!important;
  margin-left: 10px
}

.para:hover {
  color: #6A019E!important
}

.contact {
  color: #777!important
}

.contact:hover {
  color: #333!important
}

.green {
  color: #6a9d00
}

.gray {
  color: #777!important
}

h1.title {
  font-size: 3.7em;
  color: #6a9d00;
  text-align: center;
  margin-top: 50px
}

h2.subtitle {
  font-size: 2em;
  font-style: italic;
  color: #6a9d00;
  text-align: center;
  margin-bottom: 40px
}

h3.date {
  color: #444;
  font-size: 1.15em
}

h3.title {
  color: #444;
  font-size: 1.3em;
  font-weight: bold
}

h4 {
  margin-top: 30px
}

.no-bot {
  margin-bottom: 0
}

.no-top {
  margin-top: 5px
}

hr {
  height: 0.5px;
  background-color: #ccc
}

.top-20 {
  margin-top: 20px
}

.section {
  margin-left: 20px
}

.section p {
  margin-left: 20px
}

.position {
  margin-left: 20px
}

ul.no-bull {
  list-style: none
}

.right {
  text-align: right
}

.text-medium {
  font-size: 3em
}

li {
  font-size: 1em
}

.frame {
  height: 90px;
  white-space: nowrap;
  text-align: center;
  margin: 1em 0
}

.helper {
  display: inline-block;
  height: 100%;
  vertical-align: middle
}

img.vcenter {
  vertical-align: middle;
  max-height: 90px
}

.section-title {
  color: #7CA35B
}

.green {
  background-color: #7CA35B
}

.date {
  color: #7CA35B;
  color: #999;
  font-size: 1.1em;
  margin-top: 15px;
  margin-bottom: 10px;
  font-style: italic
}

ul.hor-list {
  list-style: none;
  margin: 0;
  padding: 0;
  margin-bottom: 20px;
  text-transform: uppercase;
  font-size: 0.9em;
  color: #999
}

ul.hor-list li {
  display: inline
}

ul.hor-list:before {
  content: "Tags:"
}

ul.hor-list li:after {
  content: ","
}

ul.hor-list li:last-child:after {
  content: ""
}

.capsize {
  text-transform: uppercase;
  font-family: "Georgia"!important
}

footer {
  background-color: #555;
  color: white;
  min-height: 200px;
  bottom: 0;
  width: 100%;
  float: left;
  margin-top: 50px;
  -webkit-font-smoothing: antialiased
}

footer a:link,
footer a:visited,
footer a:hover,
footer a:active {
  color: white;
  text-decoration: none
}

footer a:hover {
  color: white!important;
  text-decoration: underline
}

footer .heading {
  font-family: 'Open Sans', sans-serif!important;
  font-weight: 300;
  font-weight: 500;
  font-size: 17.6px;
  letter-spacing: 1px;
  line-height: 26.4px;
  margin-bottom: 10px;
  margin-top: 30px
}

footer .btn-blue {
  background-color: #407EB4;
  background-color: #555;
  color: white!important;
  border: solid 2px white
}

footer .btn-blue:hover {
  background-color: #407EB4;
  background-color: #333
}

.img__wrap {
  position: relative
}

.img__description_layer {
  position: absolute;
  top: 20px;
  margin-left: 20px;
  color: white;
  visibility: hidden;
  opacity: 0;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex
}

.img__wrap:hover .img__description_layer {
  visibility: visible;
  opacity: 1
}

.selected {
  color: black!important
}